    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch OrientDb Service",
            "program": "${workspaceFolder}/src/app.ts",
            "preLaunchTask": "tsc",
            "cwd": "${workspaceFolder}",
            "env": {
                "NODE_PORT":"9901"
            },
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ]
        }
    ]
}